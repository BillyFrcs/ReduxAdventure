{
  "configurations": [
    {
      "name": "Linux",
      "intelliSenseMode": "gcc-x64",
      "includePath": [
        "${workspaceFolder}/src",
        "${workspaceFolder}/lib",
        "${workspaceFolder}/test",
        "~/SFML-2.5.1/include",
        "/usr/local/include/**",
        "/usr/include/**"
      ],
      "defines": ["_DEBUG"],
      "cStandard": "c17",
      "cppStandard": "c++20",
      "forcedInclude": ["${workspaceFolder}/src/PCH.hpp"]
    },
    {
      "name": "Mac",
      "intelliSenseMode": "clang-x64",
      "compilerPath": "/usr/bin/clang",
      "macFrameworkPath": ["/Library/Frameworks", "/System/Library/Frameworks"],
      "includePath": [
        "${workspaceFolder}/src",
        "${workspaceFolder}/lib",
        "${workspaceFolder}/test",
        "/usr/local/include/**"
      ],
      "defines": ["_DEBUG"],
      "cStandard": "c17",
      "cppStandard": "c++20",
      "forcedInclude": ["${workspaceFolder}/src/PCH.hpp"]
    },
    {
      "name": "Win32",
      "intelliSenseMode": "gcc-x86",
      "compilerPath": "C:/mingw32/bin/gcc.exe",
      "includePath": [
        "${workspaceFolder}/src",
        "${workspaceFolder}/lib",
        "${workspaceFolder}/test",
        "C:/SFML-2.5.1/include"
      ],
      "defines": ["_DEBUG", "UNICODE", "_UNICODE"],
      "cStandard": "c17",
      "cppStandard": "c++20",
      "forcedInclude": ["${workspaceFolder}/src/PCH.hpp"]
    }
  ],
  "version": 4
}
